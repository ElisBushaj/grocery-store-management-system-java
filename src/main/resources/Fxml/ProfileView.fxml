<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="700.0" styleClass="profile_container" stylesheets="@../Styles/profile.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.grc.GroceryStore.Controllers.ProfileController">
   <Label prefHeight="0.0" prefWidth="95.0" text="Profile">
      <font>
         <Font size="31.0" />
      </font>
   </Label>
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <VBox prefHeight="474.0" prefWidth="300.0" styleClass="profile_form_container">
         <Label prefHeight="27.0" prefWidth="322.0" stylesheets="@../Styles/profile.css" text="Name"/>
         <TextField fx:id="name_fld"/>
         <Label prefHeight="27.0" prefWidth="308.0" text="Last Name"/>
         <TextField fx:id="last_name_fld"/>
         <Label prefHeight="27.0" prefWidth="300.0" text="Email"/>
         <TextField fx:id="email_fld"/>
         <Label prefHeight="27.0" prefWidth="307.0" fx:id="role_lb"/>
         <Button fx:id="update_btn" mnemonicParsing="false" text="Update">
            <VBox.margin>
               <Insets top="10.0"/>
            </VBox.margin>
         </Button>
      </VBox>
   </HBox>
   <padding>
      <Insets bottom="40.0" top="40.0" />
   </padding>
</VBox>
