<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox styleClass="menu_container" stylesheets="@../../Styles/menu.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.grc.GroceryStore.Controllers.Cashier.CashierMenuController">
   <AnchorPane prefHeight="600.0" VBox.vgrow="ALWAYS">
      <VBox alignment="CENTER" prefHeight="129.0" prefWidth="200.0">
         <Label prefHeight="21.0" prefWidth="121.0" text="Grocery Store" textFill="WHITE">
            <font>
               <Font size="20.0" />
            </font>
         </Label>
      </VBox>
      <VBox alignment="TOP_CENTER" layoutY="131.0" prefHeight="339.0" prefWidth="200.0">
         <Button fx:id="dashboard_btn" mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="Dashboard" />
         <Button fx:id="sell_btn" mnemonicParsing="false" text="Sell" />
         <Button fx:id="add_costumer_btn" mnemonicParsing="false" prefHeight="25.0" prefWidth="150.0" text="Add Costumer" />
      </VBox>
      <VBox alignment="CENTER" layoutY="482.0" prefHeight="118.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0">
         <Button fx:id="profile_btn" mnemonicParsing="false" prefHeight="25.0" prefWidth="235.0" text="Profile" />
         <Button fx:id="logout_btn" mnemonicParsing="false" prefHeight="25.0" prefWidth="219.0" text="Log Out" />
      </VBox>
   </AnchorPane>
</VBox>
